	expect(${1:target}).not.toEqual(${2:value});${3}
snippet notm
	expect(${1:target}).not.toMatch(${2:pattern});${3}
snippet nott
	expect(${1:target}).not.toBeTruthy();${2}
snippet notf
	expect(${1:target}).not.toBeFalsy();${2}
snippet notd
	expect(${1:target}).not.toBeDefined();${2}
