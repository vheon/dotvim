# Jasmine ---------------------------------------------------------------------
snippet des
	describe('${1:description}', function() {
		${2}
	});
snippet it
	it('${1:description}', function() {
		${2}
	});
snippet bef
	beforeEach(function() {
		${1}
	});
snippet aft
	afterEach(function() {
		${1}
	});
snippet runs
	runs(function() {
		${1}
	});
